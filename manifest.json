{
  "manifest_version": 3,
  "name": "Mon Adresse IP",
  "version": "1.2",
  "description": "Affiche votre adresse IP Internet (par Xavier Gregor)",
  "permissions": ["activeTab", "storage", "webRequest"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}

